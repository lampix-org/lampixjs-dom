    "prepublishOnly": "npm run build",
    "precommit": "lint-staged"
  },
  "keywords": [
    "lampix"
  ],
  "author": "Lampix",
  "license": "MIT",
  "devDependencies": {
    "@types/invariant": "2.2.29",
    "@types/lodash": "4.14.116",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "peer-deps-externals-webpack-plugin": "1.0.4",
    "shx": "0.3.2",
    "ts-loader": "4.4.2",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.9.2",
    "typedoc": "0.9.0",
    "typescript": "2.9.2",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0"
  },
  "lint-staged": {
    "*.ts": [
      "tslint"
    ]
  },
  "dependencies": {
    "invariant": "2.2.4",
    "lodash": "4.17.10"
  },
  "peerDependencies": {
    "@lampix/core": ">=1.0.0"
  }
